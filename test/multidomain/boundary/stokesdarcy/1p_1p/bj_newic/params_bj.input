[Darcy.Grid]
UpperRight = 1 1
Cells = 20 20

[Stokes.Grid]
LowerLeft = 0 1
UpperRight = 1 2
Cells = 20 20

[Stokes.Problem]
Name = stokes

[Darcy.Problem]
Name = darcy

[Darcy.SpatialParams]
Porosity = 1.0
Permeability_xx = 1.0 # m^2
Permeability_xy = 2.0 # m^2
Permeability_yy = 1.0 # m^2
AlphaBeaversJoseph = 1.0

[Vtk]
OutputName = test_md_boundary_stokes1p_darcy1p_bj

[Problem]
EnableGravity = false
EnableInertiaTerms = false
TestCase = BJSymmetrized
SlipCondition = BJ

[Vtk]
AddVelocity = 1

[Component]
LiquidDensity = 1.0
LiquidKinematicViscosity = 1.0

[Assembly]
NumericDifference.BaseEpsilon = 1e-4
